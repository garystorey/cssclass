{
  "name": "@garystorey/cssclass",
  "version": "1.2.0",
  "description": "Utility to build css class/className to apply to elements",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix src",
    "ci-lint": "eslint src --ext .ts,.tsx --fix --quiet",
    "prepublishOnly": "npm ci-test && npm run ci-lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "eslint": "eslint src --ext .ts,.tsx --fix",
    "push": "npm run test-hook && npm run ci-eslint && npm run ci-stylelint && npm run ci-pretty && npm run build",
    "ci-pretty": "npx prettier src --write --loglevel error",
    "test": "jest src --watch -u",
    "test-hook": "jest src -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garystorey/cssclass.git"
  },
  "keywords": [
    "css",
    "classname",
    "classes",
    "classlist",
    "combine"
  ],
  "author": "Gary Storey <garystorey655@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garystorey/cssclass/issues"
  },
  "homepage": "https://github.com/garystorey/cssclass#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@testing-library/dom": "^8.14.0",
    "@types/estree": "^0.0.52",
    "@types/jest": "^28.1.4",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "rollup": "^2.76.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "ts-jest": "^28.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "dependencies": {}
}
